{
  "rules": {
    "preset-ja-technical-writing": {
      "arabic-kanji-numbers": false,
      "no-exclamation-question-mark": false
    },
    "preset-jtf-style": {
      "3.3.かっこ類と隣接する文字の間のスペースの有無": false,
      "4.1.1.句点(。)": false,
      "4.3.1.丸かっこ（）": false
    },
    "prh": {
      "rulePaths": [
         "./dict/prh_root.yml"
      ]
    }
  }
}
